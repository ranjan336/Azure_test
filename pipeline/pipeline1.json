{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['city']"
								},
								"sink": {
									"name": "city"
								}
							},
							{
								"source": {
									"path": "$['loc'][0]"
								},
								"sink": {
									"name": "location"
								}
							},
							{
								"source": {
									"path": "$['pop']"
								},
								"sink": {
									"name": "population"
								}
							},
							{
								"source": {
									"path": "$['state']"
								},
								"sink": {
									"name": "state"
								}
							},
							{
								"source": {
									"path": "$['_id']"
								},
								"sink": {
									"name": "id"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "uspop_json",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "uspop",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}